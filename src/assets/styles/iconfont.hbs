/* stylelint-disable */
@font-face {
  font-family: "{{fontName}}";
  src: {{{src}}};
}

{{baseSelector}} {
  display: inline-block;
  transform: translate(0, 0);
  text-rendering: auto;
}

{{baseSelector}}::before {
  font-family: {{fontName}} !important;
  font-style: normal;
  font-weight: normal !important;
  vertical-align: top;
}
{{#each codepoints}}
.{{../classPrefix}}{{@key}}::before {
  content: "\\{{this}}";
}
{{/each}}
/* stylelint-enable */
